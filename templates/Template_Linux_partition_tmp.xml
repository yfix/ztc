					<status>0</status>
					<priority>4</priority>
					<comments></comments>
				</trigger>
				<trigger>
					<description>Low free disk space (&lt;1%) on {HOSTNAME} volume /tmp</description>
					<type>0</type>
					<expression>{{HOSTNAME}:vfs.fs.size[/tmp/,pfree].last(0)}&lt;1</expression>
					<url></url>
					<status>0</status>
					<priority>5</priority>
					<comments></comments>
				</trigger>
				<trigger>
					<description>Low free disk space (&lt;20%) on {HOSTNAME} volume /tmp</description>
					<type>0</type>
					<expression>{{HOSTNAME}:vfs.fs.size[/tmp/,pfree].last(0)}&lt;20</expression>
					<url></url>
					<status>0</status>
					<priority>4</priority>
